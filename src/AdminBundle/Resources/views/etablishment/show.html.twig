{% extends 'base.html.twig' %}

{% block body %}


    <div class="row">

        <div class="col s12 m12">
            <p class="justify"><i class="material-icons" style="font-size:15px; color:#e24800!important;">arrow_back</i><a style="color:#e24800;" href="{{ path('admin_etablishment_index') }}"> Retour vers la liste des établissements</a></p>
            <div class="icon-block">

                <h4 class="center" style="color:#e24800; font-weight: bold;">ShinigamiLaser - {{ etablishment.name }}</h4>

            </div>
        </div>

        <table class="responsive-table centered">
            <thead>
            <tr>
                <th>Code</th>
                <th>Ville</th>
                <th>Dernières parties jouées </th>
                <th>Scores</th>
            </tr>
            </thead>

            <tbody>
            <tr>
                <td>{{ etablishment.code }}</td>
                <td>{{ etablishment.city }}</td>


            </tr>

            </tbody>
        </table>

    </div>

{% endblock %}

